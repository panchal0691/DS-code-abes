// Online C compiler to run C program online
#include <stdio.h>


void main()
{
    int a[10],b[10],c[20];
    int  i,j,k,n,m,t;


    //enter first array
    printf("enter the num of  elements of first array");
    scanf("%d",&n);
    printf("enter the elements");
    for(i=0;i<=n;i++)
    {
        scanf("%d",&a[i]);
    }
     for(i=0;i<=n;i++)
    {
    printf("array is %d\n",a[i]);
    }

    //enter sec array


    printf("enter the num of  elements of second array");
    scanf("%d",&m);
    printf("enter the elements");
    for(i=0;i<=m;i++)
    {
        scanf("%d",&b[i]);
    }
     for(i=0;i<=m;i++)
    {
    printf("array is %d\n",b[i]);
    }


t=(n+m);
    //sorting array and mixing it
 while(i<n&&j<m)
 {

     if(a[i]<b[j])
     {
         c[k]=a[i];
         i++;
         k++;
     }
     if(a[i]==b[j])
     {
          c[k]=a[i];
         i++;
         k++;
         j++;
     }
     else(a[i]>b[j]);
     {


         c[k]=b[j];
         j++;
         k++;
         
     }
     while(i<n)
     {
         c[k]=a[i];
         i++;
         k++;
     }
      while(j<m)
     {
         c[k]=b[j];
         j++;
         k++;
     }


    

}
 for(i=0;i<=t;i++)
     {
     printf("new array is %d\n",c[i]);
     }
}











    
